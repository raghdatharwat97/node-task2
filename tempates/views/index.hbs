{{>header}}
      <div class="container">
         <div class="row">
              {{#each data.articles}}
               <div class="card mb-3 " style="border: 2px solid rgb(103, 155, 223); margin-left:auto;margin-right:auto; box-shadow:2px 2px 12px 3px rgb(166, 166, 180);margin-top:20px;">
                <img class="card-img-top img-fluid" src="{{urlToImage}}" alt="Card image cap">
                <div class="card-body text-right" style="border: 1px solid black;" >
                <h5 class="card-title">{{title}}</h5>
                <p class="card-text">{{description}}</p>
              </div>
          </div>
            {{/each}}
        </div>
      </div>
      {{>footer}}
       
